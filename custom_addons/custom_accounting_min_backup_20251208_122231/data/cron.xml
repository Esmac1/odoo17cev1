<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cron_auto_depreciation" model="ir.cron">
            <field name="name">Auto Generate Depreciation Entries</field>
            <field name="model_id" ref="model_custom_accounting_asset"/>
            <field name="state">code</field>
            <field name="code">model._cron_generate_depreciation_entries()</field>
            <field name="interval_type">months</field>
            <field name="interval_number">1</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
